---
interface Props {
    name:string;
    image:string;
}
const {name, image} = Astro.props as Props
---
<div class="flex flex-col bg-amber-900 w-40 h-40 gap-4">
    <div class="rounded-full bg-blue-900 w-14 h-14">
        <img src={image} alt="">
    </div>
    <div class="bg-blue-500 w-auto h-6">
        <p>
            {name}
        </p>
    </div>
</div>